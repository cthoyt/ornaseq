[Term]
id: ORNASEQ:0000005
name: Illumina TruSeq Rapid SBS Kit v2
is_a: GENEPIO:0000081
relationship: OBI:0000312 OBI:0000094
property_value: definition "A processed material that is a sequencing kit used with Illumina HiSeq 2500 and 1500 sequencers allowing for single-end or paired-end sequencing." xsd:string
property_value: IAO:0000117 "PERSON: Junhyong Kim" xsd:string
property_value: IAO:0000117 "PERSON: Stephen A. Fisher" xsd:string
property_value: IAO:0000119 https://www.illumina.com/products/by-type/sequencing-kits/cluster-gen-sequencing-reagents/hiseq-rapid-sbs-kit.html xsd:string

[Term]
id: ORNASEQ:0000006
name: Illumina TruSeq Stranded
is_a: GENEPIO:0000085
relationship: OBI:0000312 OBI:0000094
property_value: definition "A processed material that is used to create a sequencing library that retains mRNA strand orientation." xsd:string
property_value: IAO:0000117 "PERSON: Junhyong Kim" xsd:string
property_value: IAO:0000117 "PERSON: Stephen A. Fisher" xsd:string
property_value: IAO:0000119 https://www.illumina.com/products/by-type/sequencing-kits/library-prep-kits/truseq-stranded-mrna.html xsd:string

