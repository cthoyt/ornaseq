[Term]
id: ORNASEQ:0000001
name: date of specimen amplification
is_a: OBI:0002471
relationship: RO:0002223 ORNASEQ:0000003 ! enzymatic amplification protocol
property_value: definition "A date process started item that indicates when a specimen was run through the amplification protocol prior to library preparation and sequencing." xsd:string
property_value: IAO:0000117 "PERSON: Junhyong Kim" xsd:string
property_value: IAO:0000117 "PERSON: Stephen A. Fisher" xsd:string
property_value: IAO:0000118 "amplified date" xsd:string

