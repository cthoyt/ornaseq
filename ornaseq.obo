[Term]
id: ORNASEQ:0000008
name: number of flow cell lanes
is_a: IAO:0000027
property_value: definition "A data item that indicates the number of lanes in a flow cell that were used to sequence a specimen" xsd:string
property_value: IAO:0000117 "PERSON: Junhyong Kim" xsd:string
